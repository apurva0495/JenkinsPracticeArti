pipeline{
	agent any
        
	stages{
		stage('Build'){
			steps{
				bat 'mvn compile'
			}
		}
		stage('Test Execution'){
			parallel{
			     stage('Run Tests'){
					steps{
						bat 'mvn test'
					}
				}	
			}
		}


	}
	parallel{
		

	}

	post{
		always{
 		echo 'I will always execute'
		}
		success{
		  echo 'this is success'
		}
		
	}




}