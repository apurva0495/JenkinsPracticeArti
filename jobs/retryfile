pipeline {
    agent any
    environment {
        NAME = 'AA'
        LASTNAME = 'AG'
    }

    stages {
        stage('Build') {
            steps {
                bat 'echo %NAME% %LASTNAME%'
            }
        }
    }
    post{
        always{
            echo 'I will always execute'
        }
        success{
            echo 'I will execute if i am passed'
        }

       failure{
            echo 'I will execute if i am failed'
        }



        }
}
